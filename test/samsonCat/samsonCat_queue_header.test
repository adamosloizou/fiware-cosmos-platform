--NAME--
samsonCat - queue_header
--SHELL--
#delilah -command remove_all_stream 2> /dev/null
delilah -command remove_all_stream 
samsonPush input < input.txt
sleep 1
delilah -command "run txt.parser_words input words" 
sleep 1
delilah -command "run txt.word_count_accumulate words words_count words_count" 
sleep 1
delilah -command "pop words_count /tmp/words_count -force"
samsonCat -header /tmp/words_count | sed 's/\(\[[0-9]* [0-9]*\]\)/[***** *****]/'
--EXPECT--
