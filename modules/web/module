
Module web
{
	title	"Hit manipulation"
	author	"Andreu Urruela & Richard Gass"
	version "0.1"
}


parser parse_web_hits
{
    out system.String system.UInt

}

script init_stream_operations
{
    code
    {
		rm_stream_operation hit.04.input_words -f;
		add_stream_operation web.parse_web_hits web.parse_web_hits web.01.input.txt hit.04.input_words;
    }

    helpLine "Init stream operations"
}

script demo
{
    code
    {
		remove_all_stream;
		init_stream web.init_stream_operations;
		init_stream hit.init_stream_operations;
    }

    helpLine "Init stream operations"
}


