Module example
{
	title	"Example module to play with samson"
	author	"Andreu Urruela"
	version "0.1"
}


generator generator
{
	out base.UInt base.UInt	
	file ModuleImpl.cpp
}

map map
{
	in base.UInt base.UInt
	out base.UInt base.UInt

	file ModuleImpl.cpp

	help
	{
		"Simple map"
	}
}

reduce r
{
	in base.UInt base.UInt2
	out base.UInt system.Void

	file ModuleImpl.cpp

	helpLine	"This is another example of what it is possible ;"

}

script s
{
	out base.UInt base.UInt

	helpLine     "This if the first script of the samson platform"

	code
	"
		add_queue tmp base.UInt base.UInt

		example.generator tmp
		example.map tmp $1

	"
}
