--NAME--
push_module - upload a module
--SHELL-INIT--
mv $SAMSON_HOME/modules/libgg.so /tmp/libgg.so
delilah ${SAMSON_LOG_LEVELS} -log_port ${SAMSON_LOG_PORT_ENV} ${SAMSON_TEST_HOST} ${SAMSON_WORKER_PORT_ENV} -command reload_modules
--SHELL--
delilah ${SAMSON_LOG_LEVELS} -log_port ${SAMSON_LOG_PORT_ENV} ${SAMSON_TEST_HOST} ${SAMSON_WORKER_PORT_ENV} -command "push_module /tmp/libgg.so web"
if [ -f $SAMSON_HOME/modules/libgg.so ]; then
    echo "Module web has been uploaded"
else
    echo "Module web not found in $SAMSON_HOME/modules/libgg.so"
fi
--EXPECT--
Module web has been uploaded
