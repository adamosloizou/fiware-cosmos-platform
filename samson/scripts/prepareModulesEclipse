module=`echo ${PWD} | sed 's:^..*/::'`
direclipse=${module}_eclipse_proj

mkdir -p ${direclipse}
cd ${direclipse} 2> /dev/null
if [ "$?" != 0 ]
then
	echo "You need be on module to build"
	exit 1
fi

export PATH=/usr/local/bin:$PATH
#cp ../CMakeLists.txt .
samsonModuleParser ../module ../ Module
cmake .. -DCMAKE_BUILD_TYPE=DEBUG -G "Eclipse CDT4 - Unix Makefiles"

cd ..

