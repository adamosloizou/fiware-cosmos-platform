<workflow-app xmlns="uri:oozie:workflow:0.1" xmlns:xi="http://www.w3.org/2001/XInclude" name="webprofilingWF">
    <start to="filtering"/>
    <!-- This action runs the filtering map process over the raw data -->
    
	<fork name="aggregation">
    </fork>
	
	<!-- These actions run map reduce operations to calculate kpis of web profiling -->
	
	
	<join name="joining" to="end"/>
	
		
    <kill name="fail">
        <message>Map/Reduce failed, error message[${wf:errorMessage(wf:lastErrorNode())}]</message>
    </kill>
	
    <end name="end"/>
</workflow-app>

