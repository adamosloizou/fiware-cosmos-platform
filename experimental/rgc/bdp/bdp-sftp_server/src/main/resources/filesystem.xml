<?xml version="1.0" encoding="UTF-8"?>
<descriptions>
	<description path="/">
		<users>
			<user user="renero" isCompressible="true" isReadble="true"
				isWritable="true">
				<parser className="es.tid.bdp.utils.parse.ParserCdr"
					pattern="(^.+)\\|(.*)\\|\\d\\|(\\d{2})/(\\d{2})/(\\d{4})\\|(\\d{2}):(\\d{2}):(\\d{2})\\|.*"
					attr="userId|cellId|day|month|year|hour|minute|second" />
			</user>
			<user user="hdfs" isCompressible="false" isReadble="true"
				isWritable="true" />
			<user user="javierb" isCompressible="false" isReadble="true"
				isWritable="true">
				<parser className="es.tid.bdp.utils.parse.ParserWebLog"
					pattern="(^.+) (.*) (.*) (.*) (.*) (.*) (.*)"
					attr="visitorId|fullUrl|date|httpStatus|mimeType|userAgent|method" />
			</user>
		</users>
	</description>
</descriptions>
